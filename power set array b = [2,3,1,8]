#include <stdio.h>

int main() {
    int b[] = {2, 3, 1, 8};
    int n = 4;

    int total = 1;
    int i, j;

    
    for(i = 0; i < n; i++) {
        total = total * 2;
    }

    
    int binary[4] = {0};

    for(i = 0; i < total; i++) {

        
        printf("{ ");
        for(j = 0; j < n; j++) {
            if(binary[j] == 1) {
                printf("%d ", b[j]);
            }
        }
        printf("}\n");

        
        for(j = n - 1; j >= 0; j--) {
            if(binary[j] == 0) {
                binary[j] = 1;
                break;
            } else {
                binary[j] = 0;
            }
        }
    }

    return 0;
}
