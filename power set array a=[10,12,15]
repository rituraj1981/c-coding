#include <stdio.h>

int main() {
    int a[] = {10, 12, 15};
    int n = 3;

    int subset[3] = {0};   

    while (1) {
    
        printf("{ ");
        for (int i = 0; i < n; i++) {
            if (subset[i] == 1) {
                printf("%d ", a[i]);
            }
        }
        printf("}\n");

        
        int i = n - 1;
        while (i >= 0 && subset[i] == 1) {
            subset[i] = 0;
            i--;
        }

        if (i < 0) {
            break;   
        }

        subset[i] = 1;
    }

    return 0;
}

